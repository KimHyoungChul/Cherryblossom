<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:orientation="horizontal"
    android:clipChildren="false"
    >


    <TextView
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:text="@string/title_category1"
        android:id="@+id/textViewCategory"
        android:layout_marginLeft="46dp"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:background="#00000000"
        android:autoText="false"
        android:typeface="normal"
         />

    <TextView
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:text="@string/title_category_under1"
        android:id="@+id/textViewCategory_Under"
        android:layout_below="@id/textViewCategory"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="46dp"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:background="#00000000"
        android:autoText="false" />



    <LinearLayout
        android:orientation="vertical"
        android:layout_width="610dp"
        android:layout_height="wrap_content"
        android:clipChildren="false"
        android:id="@+id/layoutAppsLines"
        android:layout_marginLeft="65dp"
        android:background="@drawable/draw_list"
        android:layout_toRightOf="@id/textViewCategory"
        >

    </LinearLayout>



</RelativeLayout>
