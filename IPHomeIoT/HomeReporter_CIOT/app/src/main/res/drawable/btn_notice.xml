<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <corners android:radius="24dip" />
            <size android:width="@dimen/notice_call_security_width"
                android:height="@dimen/notice_call_security_height"/>
            <stroke
                android:width="@dimen/notice_call_security_stroke" android:color="#5c5a7d"/>
        </shape>
    </item>

    <item android:state_focused="true" >
        <shape android:shape="rectangle">
            <corners android:radius="24dip" />
            <size android:width="@dimen/notice_call_security_width"
                android:height="@dimen/notice_call_security_height"/>
            <stroke
                android:width="@dimen/notice_call_security_stroke" android:color="#5c5a7d"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="24dip" />
            <size android:width="@dimen/notice_call_security_width"
                android:height="@dimen/notice_call_security_height"/>
            <stroke
                android:width="@dimen/notice_call_security_stroke" android:color="#6f6c96"/>
        </shape>
    </item>

</selector>